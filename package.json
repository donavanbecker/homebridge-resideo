{
  "private": true,
  "displayName": "Homebridge Honeywell Home",
  "name": "homebridge-honeywell-home",
  "version": "6.0.0",
  "description": "A Honeywell Home https://honeywellhome.com plugin for Homebridge: https://github.com/homebridge/homebridge.",
  "author": "donavanbecker",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/donavanbecker/homebridge-honeywell-home.git"
  },
  "bugs": {
    "url": "https://github.com/donavanbecker/homebridge-honeywell-home/issues"
  },
  "engines": {
    "node": ">=10.17.0",
    "homebridge": ">=1.0.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "developer": "DEBUG=honeywell-home homebridge -D -U `pwd` -P `pwd`",
    "lint": "eslint src/**.ts",
    "watch": "npm run build && npm link && nodemon",
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "keywords": [
    "homebridge-plugin",
    "honeywell",
    "honeywell-home",
    "thermostat"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "rxjs": "^6.6.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.14",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "eslint": "^7.3.1",
    "homebridge": "^1.1.1",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5",
    "typescript-axios-wb": "^1.0.3"
  }
}
